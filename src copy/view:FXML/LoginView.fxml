<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Loginwindow" opacity="0.98" prefHeight="389.0" prefWidth="600.0" styleClass="mainWindow" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="labelWindow" layoutX="-2.0" layoutY="-2.0" prefHeight="400.0" prefWidth="600.0" />
      <ImageView fx:id="imageWindow" fitHeight="408.0" fitWidth="600.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true" styleClass="imageWindow">
         <image>
            <Image url="@back2.png" />
         </image>
      </ImageView>
      <Label fx:id="labelborder" layoutX="111.0" layoutY="107.0" prefHeight="46.0" prefWidth="48.0" />
      <Label layoutX="20.0" layoutY="18.0" prefHeight="89.0" prefWidth="152.0">
         <graphic>
            <ImageView fitHeight="88.0" fitWidth="91.0" pickOnBounds="true" preserveRatio="true">
<!--               <image>-->
<!--                  <Image url="@login%20icon.png" />-->
<!--               </image>-->
            </ImageView>
         </graphic>
      </Label>
      <ImageView fitHeight="59.0" fitWidth="46.0" layoutX="183.0" layoutY="95.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@EMAIL.png" />
         </image>
      </ImageView>
      <Label layoutX="164.0" layoutY="239.0" prefHeight="61.0" prefWidth="65.0" />
      <ImageView fitHeight="46.0" fitWidth="48.0" layoutX="183.0" layoutY="175.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@keyicon.png" />
         </image>
      </ImageView>
      <PasswordField fx:id="passwordField" layoutX="260.0" layoutY="185.0" prefHeight="30.0" prefWidth="206.0" promptText="Password" styleClass="fields" />
      <TextField fx:id="textField" layoutX="261.0" layoutY="103.0" prefHeight="30.0" prefWidth="206.0" promptText="Email" styleClass="fields" />
      <Label layoutX="440.0" layoutY="272.0" prefHeight="38.0" prefWidth="48.0" />
      <Button fx:id="loginButton" onAction="#loginButtonAction" layoutX="183.0" layoutY="281.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="135.0" styleClass="btn-warning, btn-danger" text="Log In" textFill="#0a0a0a" >
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button fx:id="exitbutton" layoutX="386.0" layoutY="281.0" mnemonicParsing="false"  prefHeight="38.0" prefWidth="78.0" styleClass="btn-warning, btn-danger" text="Exit">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label layoutX="33.0" layoutY="15.0" prefHeight="113.0" prefWidth="115.0">
         <graphic>
            <ImageView fitHeight="111.0" fitWidth="113.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@loginface.png" />
               </image></ImageView>
         </graphic>
      </Label>
   </children>
