            <Font name="System Bold" size="14.0" />
         </font></TextField>
      <Label fx:id="timeLabel" layoutX="753.0" layoutY="9.0" prefHeight="29.0" prefWidth="229.0" styleClass="textF" textFill="#ebe7e7">
