<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="520.0" prefWidth="990.0" styleClass="background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="511.0" fitWidth="976.0" layoutX="7.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" styleClass="border">
         <image>
            <Image url="@images/color.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="table" layoutX="26.0" layoutY="185.0" prefHeight="296.0" prefWidth="926.0" styleClass="textF">
        <columns>
            <TableColumn fx:id="col_id" prefWidth="41.0" text="Id" />
          <TableColumn fx:id="col_mediaType" prefWidth="97.0" text="Media Type" />
          <TableColumn fx:id="col_tytleName" prefWidth="193.0" text="Title Name" />
            <TableColumn fx:id="col_genre" prefWidth="129.0" text="Genre" />
            <TableColumn fx:id="col_director" prefWidth="181.0" text="Director" />
            <TableColumn fx:id="col_year" text="Year" />
            <TableColumn fx:id="col_price" prefWidth="58.0" text="Price" />
            <TableColumn fx:id="col_totalQty" prefWidth="72.0" text="Total Qty" />
            <TableColumn fx:id="col_availableQty" prefWidth="73.0" text="Available Qty" />
        </columns>
      </TableView>
      <TextField fx:id="titleField" layoutX="197.0" layoutY="54.0" prefHeight="27.0" prefWidth="142.0" promptText="Title Name" styleClass="textF" />
      <TextField fx:id="genreField" layoutX="368.0" layoutY="54.0" prefHeight="27.0" prefWidth="142.0" promptText="Genre" styleClass="textF" />
      <TextField fx:id="priceField" layoutX="197.0" layoutY="93.0" prefHeight="27.0" prefWidth="142.0" promptText="Price" styleClass="textF" />
      <Button fx:id="additemButton" layoutX="746.0" layoutY="58.0" mnemonicParsing="false" onAction="#addOnClick" prefHeight="54.0" prefWidth="117.0" styleClass="lightgreen" text="Add Item">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label layoutX="26.0" layoutY="144.0" prefHeight="29.0" prefWidth="213.0" text="Stock Control" textFill="#110000">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="goBackButton" layoutX="294.0" layoutY="136.0" mnemonicParsing="false" onAction="#goBackOnClick" styleClass="customerButton" text="Go Back" />
      <Button fx:id="deleteButton" layoutX="430.0" layoutY="136.0" mnemonicParsing="false" onAction="#deleteOnClick" styleClass="danger" text="Delete" />
      <TextField fx:id="totalQuantityField" layoutX="368.0" layoutY="93.0" prefHeight="27.0" prefWidth="142.0" promptText="Total Quantity" styleClass="textF" />
      <TextField fx:id="year_field" layoutX="27.0" layoutY="91.0" prefHeight="27.0" prefWidth="142.0" promptText="Year Of Release" styleClass="textF" />
      <Line endX="204.0" endY="9.0" layoutX="752.0" layoutY="118.0" startX="-726.0" startY="9.0" stroke="#000d01" strokeWidth="2.0" />
