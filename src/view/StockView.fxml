            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="tlabel" layoutX="50.0" layoutY="14.0" prefHeight="18.0" prefWidth="229.0" />
      <TextField fx:id="directorField" layoutX="531.0" layoutY="54.0" prefHeight="27.0" prefWidth="142.0" promptText="Director" styleClass="textF" />
      <TextField fx:id="quantityField" layoutX="27.0" layoutY="54.0" prefHeight="27.0" prefWidth="142.0" promptText="Quantity Available" styleClass="textF" />
      <ChoiceBox fx:id="mediaChoice" layoutX="531.0" layoutY="91.0" prefHeight="29.0" prefWidth="142.0" />
