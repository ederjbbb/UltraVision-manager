         <font>
            <Font size="10.0" />
         </font>
         <graphic>
            <ImageView fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/logoutButton.png" />
               </image>
            </ImageView>
         </graphic></Button>
